<%= render 'public/customers/header' %>

<h3>配送先編集</h3>
<%= form_with model: @address, url: address_path(@address.id) do |f| %>

<h5>郵便番号（ハイフンなし）</h5>
<%= f.text_field :postal_code %>

<h5>住所</h5>
<%= f.text_field :address %>

<h5>宛名</h5>
<%= f.text_field :name %>

<%= f.submit '編集内容を保存' %>
<% end %>