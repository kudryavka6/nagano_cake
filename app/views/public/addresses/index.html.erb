<%= 'render/customer/header' %>

<h3>配送先登録 / 一覧</h3>
<%= form_with do |f| %>

  <h5>郵便番号（ハイフンなし）</h5>
  <%= f.text_field :postal_code %>

  <h5>住所</h5>
  <%= f.text_field :address %>

  <h5>宛名</h5>
  <%= f.text_field :name %>

  <%= submit '新規登録' %>

<% end %>

